package variables;

public class myschedule 
{
	public static void main( String[] args )
	{
		String per_one, per_two, per_three, per_four, nameone, name, namethree;
		
		per_one = "English";
		per_two = "Computer Science";
		per_three = "Biology";
		per_four = "Principles of Engineering";
		nameone = "Thow";
		name = "Mauro";
		namethree = "Slijk";
		
		System.out.println("Miranda's Class Schedule:");
		System.out.println("");
		System.out.println("---------------------------------------");
		System.out.println("|Per 1 |   " + per_one + "       |   " + nameone + "   |");
		System.out.println("|Per 2 |   " + per_two + "    |   " + name + "   |");
		System.out.println("|Per 3 |   " + per_three + "       |   " + namethree + "   |");
		System.out.println("|Per 4 |   " + per_four + "   |   " + name + "   |");
		System.out.println("----------------------------------------");
	}

}
